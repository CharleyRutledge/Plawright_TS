{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "files.associations": {
        "*.spec.ts": "typescript"
    },
    "files.exclude": {
        "**/node_modules": true,
        "**/test-results": true,
        "**/playwright-report": true,
        "**/.auth": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/test-results": true,
        "**/playwright-report": true
    }
}